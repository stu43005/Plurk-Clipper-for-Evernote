{
    "manifest_version": 2,
    "name": "Plurk Clipper for Evernote",
    "version": "0.1.0",
    "description": "Clip Plurk's content to Evernote.",
    "icons": {
        "128": "128.png",
        "16": "16.png"
    },
    "author": "Shiaupiau",
    "content_scripts": [
        {
            "matches": [
                "http://www.plurk.com/*",
                "https://www.plurk.com/*"
            ],
            "js": [
                "libs/jsOAuth-1.3.7.min.js",
                "libs/evernote-sdk-minified.js",
                "config.js",
                "content-script.js"
            ]
        }
    ],
    "permissions": [
        "http://www.plurk.com/",
        "https://www.plurk.com/"
    ]
}